<h1>New customer</h1>

<%= form_for(@customer) do |f| %>

	  <div class="field">
	    <%= f.label :contact_info %>
	    <%= collection_select :customer, :contact_info_id, ContactInfo.all, :id, :FirstName %>
	  </div>

	    <div class="field">
    		<%= f.label :Club %>
    		<%= collection_select :customer, :club_id, Club.all, :id, :ClubName %>
  		</div>

	  <div class="actions">
	    <%= f.submit %>
	  </div>
<% end %>