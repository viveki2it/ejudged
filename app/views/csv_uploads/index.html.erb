<h1>Listing CSV uploads</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Company</th>
    <th></th>
    <th></th>
  </tr>

<% @csv_uploads.each do |csv_upload| %>
  <tr>
    <td><%= csv_upload.Type %></td>
    <%if csv_upload.company != nil%>
      <td><%= csv_upload.company.Name %></td>
    <%else%>
      <td>Unassigned</td>
    <%end%>
    <td><%= link_to 'Show', csv_upload %></td>
    <td><%= link_to 'Destroy', csv_upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Csv upload', new_csv_upload_path %>
